{
  "name": "edgeworkers-response-transform-template",
  "version": "1.0.0",
  "description": "EdgeWorkers sample: Response Transform app template",
  "main": "main.js",
  "author": "Shige Fukushima",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "prettier": "prettier --write src/**/*.ts tests/**/*.ts",
    "start": "rollup -c -w",
    "build": "rollup -c && yarn bundle:build",
    "bundle:build": "npm-run-all bundle:copyjson bundle:replace bundle:rmtarball bundle:tarball",
    "bundle:copyjson": "cp src/bundle.json dist/bundle.json",
    "bundle:replace": "./replace dist/main.js",
    "bundle:rmtarball": "rimraf dist/ew-bundle.tgz",
    "bundle:tarball": "cd dist && tar -zcvf ew-bundle.tgz bundle.json main.js",
    "clean": "rimraf ./dist",
    "test": "jest"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/akamai-edgeworkers": "^1.0.8",
    "@types/jest": "^27.4.0",
    "@types/node": "16",
    "@types/node-fetch": "2",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.4.0",
    "jest": "27.4",
    "node-fetch": "2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.2",
    "shx": "^0.3.4",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "typescript": "4.4"
  },
  "dependencies": {}
}
